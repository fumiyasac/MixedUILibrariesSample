ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/DeckTransition" "$PODS_CONFIGURATION_BUILD_DIR/FSPagerView" "$PODS_CONFIGURATION_BUILD_DIR/FlexibleSteppedProgressBar" "$PODS_CONFIGURATION_BUILD_DIR/Hero" "$PODS_CONFIGURATION_BUILD_DIR/VegaScrollFlowLayout"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/DeckTransition/DeckTransition.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/FSPagerView/FSPagerView.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/FlexibleSteppedProgressBar/FlexibleSteppedProgressBar.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Hero/Hero.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/VegaScrollFlowLayout/VegaScrollFlowLayout.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "DeckTransition" -framework "FSPagerView" -framework "FlexibleSteppedProgressBar" -framework "Hero" -framework "VegaScrollFlowLayout"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
